<h1>Wikinonymous</h1>
<p><%= link_to "New Article", new_article_path %></p>

<h1>Editing Article</h1>
<%= form_for @article do |f|%>
<%= f.label :title%>
<%= f.text_field :title, :value => @article.title %>
<%= f.label :content %>
<%= f.text_field :content, :value => @article.content %>
<%= f.submit %>
<% end %>

<p> <%= link_to 'Show', article_path(@article) %> | <%= link_to 'Back', articles_path %></p>
